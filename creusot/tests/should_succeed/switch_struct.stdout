module Ambient
  use Ref
  use mach.int.Int
  use mach.int.Int32
  use mach.int.Int64
  use mach.int.UInt32
  use mach.int.UInt64
  use string.Char
  use floating_point.Single
  use floating_point.Double

  type usize = uint64
  type isize = int64

  type borrowed 'a = { current : 'a ; final : 'a; }
  let function ( *_ ) x = x.current
  let function ( ^_ ) x = x.final
  val borrow_mut (a : 'a) : borrowed 'a
  ensures { *result = a }

  predicate drop_fix (_ : 'a) = true
  predicate drop_uint (_ : 'a) = true
  predicate drop_int (_ : 'a) = true
  predicate drop_float (_ : 'a) = true
  predicate drop_bool (_ : 'a) = true
  predicate drop_mut_ref (b : borrowed 'a) = ^ b = * b
  predicate drop_ref (_ : 'a) = true

  scope Type
  type m 't =
      | M_F(uint32)
      | M_G('t)

  predicate drop_m (drop_'t : 't -> bool) (self : m 't) =
    match (self) with
      | M_F(a) -> drop_uint a
      | M_G(a) -> drop_'t a
      end

  end
  let rec cfg test (o_o_1 : Type.m uint32) : bool
    =
  var _0 : bool;
  var o_1 : Type.m uint32;
  var _2 : isize;
  var field1_3 : uint32;
  var _4 : uint32;
  var field2_5 : uint32;
  var _6 : uint32;
  {
    o_1 <- o_o_1;
    goto BB0
  }
  BB0 {
    switch (o_1)
      | Type.M_F(_) -> goto BB1
      | Type.M_G(_) -> goto BB2
      | _ -> goto BB3
      end
  }
  BB1 {
    assume { drop_int _2 };
    goto BB4
  }
  BB2 {
    assume { drop_int _2 };
    field2_5 <- (let Type.M_G(a) = o_1 in a);
    assume { Type.drop_m drop_uint o_1 };
    _6 <- field2_5;
    assume { drop_uint field2_5 };
    _0 <- _6 = (0 : uint32);
    goto BB5
  }
  BB3 {
    assume { Type.drop_m drop_uint o_1 };
    assume { drop_int _2 };
    absurd
  }
  BB4 {
    field1_3 <- (let Type.M_F(a) = o_1 in a);
    assume { Type.drop_m drop_uint o_1 };
    _4 <- field1_3;
    assume { drop_uint field1_3 };
    _0 <- _4 > (0 : uint32);
    goto BB5
  }
  BB5 {
    return _0
  }


  let rec cfg main () : ()
    =
  var _0 : ();
  {
    goto BB0
  }
  BB0 {
    _0 <- ();
    return _0
  }


end
