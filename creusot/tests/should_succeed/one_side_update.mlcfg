module Ambient
  use Ref
  use mach.int.Int
  use mach.int.Int32
  use mach.int.Int64
  use mach.int.UInt32
  use mach.int.UInt64
  use string.Char
  use floating_point.Single
  use floating_point.Double

  type usize = int
  type isize = int

  type borrowed 'a = { current : 'a ; final : 'a; }
  let function ( *_ ) x = x.current
  let function ( ^_ ) x = x.final
  val borrow_mut (a : 'a) : borrowed 'a
  ensures { *result = a }

  predicate drop_fix (_ : 'a) = true
  predicate drop_uint (_ : 'a) = true
  predicate drop_int (_ : 'a) = true
  predicate drop_float (_ : 'a) = true
  predicate drop_bool (_ : 'a) = true
  predicate drop_mut_ref (b : borrowed 'a) = ^ b = * b
  predicate drop_ref (_ : 'a) = true

  scope Type
  type myint  =
      | Myint_MyInt(usize)

  predicate drop_myint (self : myint) =
    let Myint_MyInt(a) = self in drop_uint a
  end
  let rec cfg main () : ()
    =
  var _0 : ();
  var a_1 : Type.myint;
  var b_2 : borrowed (Type.myint);
  var _3 : bool;
  var _4 : bool;
  var _5 : usize;
  var _6 : Type.myint;
  {
    goto BB0
  }
  BB0 {
    a_1 <- Type.Myint_MyInt(10);
    b_2 <- borrow_mut a_1;
    a_1 <-  ^ b_2;
    _3 <- true;
    switch (_3)
      | False -> goto BB2
      | True -> goto BB1
      | _ -> goto BB1
      end
  }
  BB1 {
    assume { drop_mut_ref b_2 };
    assume { drop_bool _3 };
    _5 <- (let Type.Myint_MyInt(a) = a_1 in a);
    assume { Type.drop_myint a_1 };
    _4 <- _5 = 10;
    assume { drop_bool _4 };
    _0 <- ();
    goto BB3
  }
  BB2 {
    assume { Type.drop_myint a_1 };
    assume { drop_bool _3 };
    _6 <- Type.Myint_MyInt(5);
    b_2 <- { b_2 with current = _6 };
    assume { drop_mut_ref b_2 };
    assume { Type.drop_myint _6 };
    _0 <- ();
    goto BB3
  }
  BB3 {
    return _0
  }


end
