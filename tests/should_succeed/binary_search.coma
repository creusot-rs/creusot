    | & _58: UInt32.t = Any.any_l ()
    | & _61: bool = Any.any_l ()
    | & _65: bool = Any.any_l ()
    | & _68: UInt64.t = Any.any_l () ])
    [ return (result: t_Result_usize_usize) -> {[@expl:binary_search ensures #0] forall x: UInt64.t. result = Ok x
        -> get_u32 arr (UInt64.t'int x) = Some elem}
      {[@expl:binary_search ensures #1] forall x: UInt64.t. result = Err x
        -> (forall i: UInt64.t. 0 <= UInt64.t'int i /\ UInt64.t'int i < UInt64.t'int x
          -> UInt32.le (get_default_u32 arr (UInt64.t'int i) (0: UInt32.t)) elem)}
      {[@expl:binary_search ensures #2] forall x: UInt64.t. result = Err x
        -> (forall i: UInt64.t. UInt64.t'int x < UInt64.t'int i /\ UInt64.t'int i < len_u32 arr
          -> UInt32.lt elem (get_default_u32 arr (UInt64.t'int i) (0: UInt32.t)))}
      (! return {result}) ]
end
