module M_unnest
  use creusot.prelude.MutBorrow
  use creusot.int.UInt32
  use creusot.prelude.Any
  
  predicate resolve_refmut_u32 [@inline:trivial] (_1: MutBorrow.t UInt32.t) = _1.final = _1.current
  
  meta "rewrite_def" predicate resolve_refmut_u32
  
  predicate resolve_refmut_refmut_u32 [@inline:trivial] (_1: MutBorrow.t (MutBorrow.t UInt32.t)) = _1.final = _1.current
  
  meta "rewrite_def" predicate resolve_refmut_refmut_u32
  
  meta "compute_max_steps" 1000000
  
  meta "select_lsinst" "all"
  
  let rec unnest (x: MutBorrow.t (MutBorrow.t UInt32.t)) (return (x'0: MutBorrow.t UInt32.t)) = (! bb0
    [ bb0 = s0
      [ s0 = MutBorrow.borrow_mut <UInt32.t> {x.current.current}
          (fun (_bor: MutBorrow.t UInt32.t) ->
            [ &_2 <- _bor ] [ &x <- { x with current = { x.current with current = _bor.final } } ] s1)
      | s1 = MutBorrow.borrow_final <UInt32.t> {_2.current} {MutBorrow.get_id _2}
          (fun (_bor: MutBorrow.t UInt32.t) -> [ &_ret <- _bor ] [ &_2 <- { _2 with current = _bor.final } ] s2)
      | s2 = -{resolve_refmut_u32 _2}- s3
      | s3 = -{resolve_refmut_refmut_u32 x}- s4
      | s4 = return {_ret} ] ]
    [ & _ret: MutBorrow.t UInt32.t = Any.any_l ()
    | & x: MutBorrow.t (MutBorrow.t UInt32.t) = x
    | & _2: MutBorrow.t UInt32.t = Any.any_l () ])
    [ return (result: MutBorrow.t UInt32.t) -> {[@expl:unnest ensures #0] result.current = x.current.current}
      {[@expl:unnest ensures #1] result.final = x.final.current}
      {[@expl:unnest ensures #2] x.current.final = x.final.final}
      (! return {result}) ]
end
