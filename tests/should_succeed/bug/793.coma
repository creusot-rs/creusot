module M_div_err
  use creusot.prelude.Any
  use creusot.float.Float64
  use creusot.int.UInt32
  use mach.int.Int
  
  constant const_MAX: UInt32.t = (4294967295: UInt32.t)
  
  meta "compute_max_steps" 1000000
  
  meta "select_lsinst" "all"
  
  let rec div_err (f: Float64.t) (x: UInt32.t) (return (x'0: ())) = {[@expl:div_err requires] UInt32.t'int x
    < Int.div (UInt32.t'int const_MAX) 1}
    (! bb0
    [ bb0 = return {_ret} ] [ & _ret: () = Any.any_l () ]) [ return (result: ()) -> (! return {result}) ]
end
