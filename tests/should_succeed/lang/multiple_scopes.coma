module M_multiple_scopes
  use creusot.int.Int32
  use creusot.prelude.Any
  
  meta "compute_max_steps" 1000000
  
  meta "select_lsinst" "all"
  
  let rec multiple_scopes (return (x: ())) = (! bb0
    [ bb0 = s0 [ s0 = [ &_y <- (3: Int32.t) ] s1 | s1 = return {_ret} ] ]
    [ & _ret: () = Any.any_l () | & _y: Int32.t = Any.any_l () ]) [ return (result: ()) -> (! return {result}) ]
end
