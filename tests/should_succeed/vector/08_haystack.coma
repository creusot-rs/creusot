      {[@expl:search ensures #2] [%#s08_haystack'14] UInt64.t'int result = Seq.length (view'0 haystack)
       -> (forall i : int . 0 <= i /\ i < Seq.length (view'0 haystack)
       -> not match_at needle haystack i (Seq.length (view'0 needle)))}
      (! return' {result}) ]

