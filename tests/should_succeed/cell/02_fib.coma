module M_fib
  use mach.int.Int
  
  predicate well_founded_relation_Int [@inline:trivial] (self: int) (other: int) = self >= 0 /\ self > other
  
  meta "rewrite_def" predicate well_founded_relation_Int
  
  meta "compute_max_steps" 1000000
  
  meta "select_lsinst" "all"
  
  constant i : int
  
  function fib (i: int) : int
  
  goal vc_fib: if i <= 0 then
      true
    else
      if i = 1 then true else well_founded_relation_Int i (i - 1) /\ well_founded_relation_Int i (i - 2)

end
module M_lemma_fib_bound
  use mach.int.Int
  use bv.Pow2int
  
  function fib (i: int) : int
  
  axiom fib_def: forall i: int. fib i = (if i <= 0 then 0 else if i = 1 then 1 else fib (i - 1) + fib (i - 2))
  
  predicate well_founded_relation_Int [@inline:trivial] (self: int) (other: int) = self >= 0 /\ self > other
  
  meta "rewrite_def" predicate well_founded_relation_Int
  
  meta "compute_max_steps" 1000000
  
  meta "select_lsinst" "all"
  
  constant i : int
  
  function lemma_fib_bound (i: int) : ()
  
  goal vc_lemma_fib_bound: 0 <= i
    -> (if i = 0 then
      [@expl:lemma_fib_bound ensures] fib i <= Pow2int.pow2 i
    else
      if i = 1 then
        [@expl:lemma_fib_bound ensures] fib i <= Pow2int.pow2 i
      else
        (([@expl:lemma_fib_bound requires] 0 <= i - 2) /\ well_founded_relation_Int i (i - 2))
        /\ (fib (i - 2) <= Pow2int.pow2 (i - 2)
        -> (let _ = lemma_fib_bound (i - 2) in (([@expl:lemma_fib_bound requires] 0 <= i - 1)
          /\ well_founded_relation_Int i (i - 1))
        /\ (fib (i - 1) <= Pow2int.pow2 (i - 1) -> ([@expl:lemma_fib_bound ensures] fib i <= Pow2int.pow2 i))))
    
    )
end
