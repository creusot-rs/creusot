module M_forall
  predicate invariant_WithInvariant (self: ()) = true
  
  meta "compute_max_steps" 1000000
  
  meta "select_lsinst" "all"
  
  function forall' : ()
  
  goal vc_forall: [@stop_split] [@expl:forall ensures] forall x: (). invariant_WithInvariant x
end
module M_exists
  meta "compute_max_steps" 1000000
  
  meta "select_lsinst" "all"
  
  function exists' : ()
  
  goal vc_exists: [@stop_split] [@expl:exists ensures] true
end
