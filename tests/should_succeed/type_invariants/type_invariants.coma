module M_id
  use creusot.prelude.Any
  
  predicate invariant_WithInvariant (self: ()) = true
  
  predicate inv_WithInvariant [@inline:trivial] (_1: ()) = invariant_WithInvariant _1
  
  meta "rewrite_def" predicate inv_WithInvariant
  
  meta "compute_max_steps" 1000000
  
  meta "select_lsinst" "all"
  
  let rec id (x: ()) (return (x'0: ())) = {[@expl:id 'x' type invariant] inv_WithInvariant x}
    (! bb0
    [ bb0 = s0 [ s0 = [ &_ret <- x ] s1 | s1 = return {_ret} ] ] [ & _ret: () = Any.any_l () | & x: () = x ])
    [ return (result: ()) -> {[@expl:id result type invariant] inv_WithInvariant result} (! return {result}) ]
end
