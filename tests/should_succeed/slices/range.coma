  type t_RangeInclusive_usize
  
  predicate inv_RangeInclusive_usize (_1: t_RangeInclusive_usize)
  
  function start_log_RangeInclusive_usize (self: t_RangeInclusive_usize) : UInt64.t
  
  function end_log_RangeInclusive_usize (self: t_RangeInclusive_usize) : UInt64.t
  
  function deep_model_usize [@inline:trivial] (self: UInt64.t) : int = UInt64.t'int self
  
  meta "rewrite_def" function deep_model_usize
  
  predicate is_empty_log_RangeInclusive_usize (self: t_RangeInclusive_usize)
  
  axiom is_empty_log_RangeInclusive_usize_spec:
    forall self: t_RangeInclusive_usize. not is_empty_log_RangeInclusive_usize self
      -> deep_model_usize (start_log_RangeInclusive_usize self) <= deep_model_usize (end_log_RangeInclusive_usize self)
  
  let rec new_usize (start: UInt64.t) (end': UInt64.t) (return (x: t_RangeInclusive_usize)) = any
    [ return (result: t_RangeInclusive_usize) -> {inv_RangeInclusive_usize result}
      {start_log_RangeInclusive_usize result = start}
      {end_log_RangeInclusive_usize result = end'}
      {deep_model_usize start <= deep_model_usize end' -> not is_empty_log_RangeInclusive_usize result}
      (! return {result}) ]
  
  predicate in_bounds_RangeInclusive_usize (self: t_RangeInclusive_usize) (seq: Seq.seq UInt32.t)
  
  axiom in_bounds_RangeInclusive_usize_spec:
    forall self: t_RangeInclusive_usize, seq: Seq.seq UInt32.t. UInt64.t'int (end_log_RangeInclusive_usize self)
          < Seq.length seq
        /\ UInt64.t'int (start_log_RangeInclusive_usize self) <= UInt64.t'int (end_log_RangeInclusive_usize self) + 1
      -> in_bounds_RangeInclusive_usize self seq
  
  axiom in_bounds_RangeInclusive_usize_spec'0:
    forall self: t_RangeInclusive_usize, seq: Seq.seq UInt32.t. UInt64.t'int (end_log_RangeInclusive_usize self)
        >= Seq.length seq -> not in_bounds_RangeInclusive_usize self seq
  
  predicate has_value_RangeInclusive_usize (self: t_RangeInclusive_usize) (seq: Seq.seq UInt32.t) (out: Slice64.slice UInt32.t) =
    if is_empty_log_RangeInclusive_usize self then
      Slice64.view out = (Seq.empty: Seq.seq UInt32.t)
    else
      Seq.([..]) seq (UInt64.t'int (start_log_RangeInclusive_usize self)) (UInt64.t'int (end_log_RangeInclusive_usize self)
        + 1)
      = Slice64.view out
  
  
  predicate resolve_elswhere_RangeInclusive_usize (self: t_RangeInclusive_usize) (old': Seq.seq UInt32.t) (fin: Seq.seq UInt32.t) =
    forall i: int. 0 <= i
        /\ (i < UInt64.t'int (start_log_RangeInclusive_usize self)
          \/ UInt64.t'int (end_log_RangeInclusive_usize self) < i \/ is_empty_log_RangeInclusive_usize self)
        /\ i < Seq.length old' -> Seq.get old' i = Seq.get fin i
  
  let rec index_mut_slice_u32 (self_: MutBorrow.t (Slice64.slice UInt32.t)) (ix: t_RangeInclusive_usize)
    (return (x: MutBorrow.t (Slice64.slice UInt32.t))) =
    {[@expl:index_mut 'ix' type invariant] inv_RangeInclusive_usize ix}
    {[@expl:index_mut requires] in_bounds_RangeInclusive_usize ix (Slice64.view self_.current)}
    any
    [ return (result: MutBorrow.t (Slice64.slice UInt32.t)) ->
    {has_value_RangeInclusive_usize ix (Slice64.view self_.current) result.current}
      {has_value_RangeInclusive_usize ix (Slice64.view self_.final) result.final}
      {resolve_elswhere_RangeInclusive_usize ix (Slice64.view self_.current) (Slice64.view self_.final)}
      {Seq.length (Slice64.view self_.final) = Seq.length (Slice64.view self_.current)}
      (! return {result}) ]
  
  predicate resolve_ref_slice_u32 [@inline:trivial] (_1: MutBorrow.t (Slice64.slice UInt32.t)) = _1.final = _1.current
  
  meta "rewrite_def" predicate resolve_ref_slice_u32
  
  meta "compute_max_steps" 1000000
  
  meta "select_lsinst" "all"
  
