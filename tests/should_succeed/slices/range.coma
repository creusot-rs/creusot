      | s1 = [ &_14 <- (1: UInt64.t) ] s2
      | s2 = Opaque.fresh_ptr
          (fun (_ptr: Opaque.ptr) -> -{Slice64.slice_ptr_len _ptr = Slice64.length s.current}- [ &_15 <- _ptr ] s3)
      | s3 = [ &_16 <- Slice64.slice_ptr_len _15 ] s4
      | s4 = [ &_17 <- UInt64.lt _14 _16 ] s5
      | s5 = {[@expl:index in bounds] _17} s6
      | s6 = bb4 ]
    | bb4 = s0
      [ s0 = Slice64.set <UInt32.t> {s.current} {_14} {(1: UInt32.t)}
          (fun (r: Slice64.slice UInt32.t) -> [ &s <- { s with current = r } ] s1)
      | s1 = -{resolve_ref_slice_u32 s}- s2
      | s2 = -{resolve_ref_slice_u32 _7}- s3
