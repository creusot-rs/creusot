          (fun (_ret: MutBorrow.t (Slice64.slice UInt32.t)) ->
            [ &s <- _ret ] [ &_7 <- { _7 with current = _ret.final } ] s1)
      | s1 = [ &_10 <- (0: UInt64.t) ] s2
      | s2 = Opaque.fresh_ptr
