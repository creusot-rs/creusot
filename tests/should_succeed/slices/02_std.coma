module M_binary_search
  use creusot.int.UInt32
  use creusot.prelude.Any
  use creusot.slice.Slice64
  use creusot.int.UInt64
  use seq.Seq
  use mach.int.Int
  
  type t_Result_usize_usize = Ok UInt64.t | Err UInt64.t
  
  predicate sorted_range_Int (self: Seq.seq int) (start: int) (end': int) =
    forall i: int, j: int. start <= i /\ i <= j /\ j < end' -> Seq.get self i <= Seq.get self j
  
  predicate sorted_Int (self: Seq.seq int) = sorted_range_Int self 0 (Seq.length self)
  
  function deep_model_u32 [@inline:trivial] (self: UInt32.t) : int = UInt32.t'int self
  
  meta "rewrite_def" function deep_model_u32
  
  function index_slice_u32 [@inline:trivial] (self: Slice64.slice UInt32.t) (ix: int) : UInt32.t =
    Seq.get (Slice64.view self) ix
  
  meta "rewrite_def" function index_slice_u32
  
  function deep_model_slice_u32 (self: Slice64.slice UInt32.t) : Seq.seq int
  
  axiom deep_model_slice_u32_spec: forall self: Slice64.slice UInt32.t. Seq.length (Slice64.view self)
      = Seq.length (deep_model_slice_u32 self)
  
  axiom deep_model_slice_u32_spec'0: forall self: Slice64.slice UInt32.t. forall i: int. 0 <= i
          /\ i < Seq.length (deep_model_slice_u32 self)
        -> Seq.get (deep_model_slice_u32 self) i = deep_model_u32 (index_slice_u32 self i)
  
  function deep_model_ref_slice_u32 [@inline:trivial] (self: Slice64.slice UInt32.t) : Seq.seq int =
    deep_model_slice_u32 self
  
  meta "rewrite_def" function deep_model_ref_slice_u32
  
  function deep_model_ref_u32 [@inline:trivial] (self: UInt32.t) : int = deep_model_u32 self
  
  meta "rewrite_def" function deep_model_ref_u32
  
  type t_Ordering = Less | Equal | Greater
  
  function cmp_log_usize (self: UInt64.t) (o: UInt64.t) : t_Ordering = if UInt64.lt self o then
      Less
    else
      if self = o then Equal else Greater
  
  
  function eq_cmp_usize (x: UInt64.t) (y: UInt64.t) : ()
  
  axiom eq_cmp_usize_spec: forall x: UInt64.t, y: UInt64.t. (x = y) = (cmp_log_usize x y = Equal)
  
  function antisym2_usize (x: UInt64.t) (y: UInt64.t) : ()
  
  axiom antisym2_usize_spec: forall x: UInt64.t, y: UInt64.t. cmp_log_usize x y = Greater -> cmp_log_usize y x = Less
  
  function antisym1_usize (x: UInt64.t) (y: UInt64.t) : ()
  
  axiom antisym1_usize_spec: forall x: UInt64.t, y: UInt64.t. cmp_log_usize x y = Less -> cmp_log_usize y x = Greater
  
  function trans_usize (x: UInt64.t) (y: UInt64.t) (z: UInt64.t) (o: t_Ordering) : ()
  
  axiom trans_usize_spec: forall x: UInt64.t, y: UInt64.t, z: UInt64.t, o: t_Ordering. cmp_log_usize x y = o
      -> cmp_log_usize y z = o -> cmp_log_usize x z = o
  
  function refl_usize (x: UInt64.t) : ()
  
  axiom refl_usize_spec: forall x: UInt64.t. cmp_log_usize x x = Equal
  
  function cmp_gt_log_usize (x: UInt64.t) (y: UInt64.t) : ()
  
  axiom cmp_gt_log_usize_spec: forall x: UInt64.t, y: UInt64.t. UInt64.gt x y = (cmp_log_usize x y = Greater)
  
  function cmp_ge_log_usize (x: UInt64.t) (y: UInt64.t) : ()
  
  axiom cmp_ge_log_usize_spec: forall x: UInt64.t, y: UInt64.t. UInt64.ge x y = (cmp_log_usize x y <> Less)
  
  function cmp_lt_log_usize (x: UInt64.t) (y: UInt64.t) : ()
  
  axiom cmp_lt_log_usize_spec: forall x: UInt64.t, y: UInt64.t. UInt64.lt x y = (cmp_log_usize x y = Less)
  
  function cmp_le_log_usize (x: UInt64.t) (y: UInt64.t) : ()
  
  axiom cmp_le_log_usize_spec: forall x: UInt64.t, y: UInt64.t. UInt64.le x y = (cmp_log_usize x y <> Greater)
  
  let rec binary_search_u32 (self_: Slice64.slice UInt32.t) (x: UInt32.t) (return (x'0: t_Result_usize_usize)) =
    {[@expl:binary_search requires] sorted_Int (deep_model_ref_slice_u32 self_)}
    any
    [ return (result: t_Result_usize_usize) -> {forall i: UInt64.t. result = Ok i
        -> UInt64.t'int i < Seq.length (Slice64.view self_)
        /\ Seq.get (deep_model_slice_u32 self_) (UInt64.t'int i) = deep_model_ref_u32 x}
      {forall i: UInt64.t. result = Err i
        -> UInt64.t'int i <= Seq.length (Slice64.view self_)
        /\ (forall j: int. 0 <= j /\ j < Seq.length (Slice64.view self_)
          -> Seq.get (deep_model_ref_slice_u32 self_) j <> deep_model_ref_u32 x)}
      {forall i: UInt64.t. result = Err i
        -> (forall j: UInt64.t. UInt64.lt j i
          -> Seq.get (deep_model_ref_slice_u32 self_) (UInt64.t'int j) < deep_model_ref_u32 x)}
      {forall i: UInt64.t. result = Err i
        -> (forall j: UInt64.t. UInt64.le i j /\ UInt64.t'int j < Seq.length (Slice64.view self_)
          -> deep_model_ref_u32 x < Seq.get (deep_model_ref_slice_u32 self_) (UInt64.t'int j))}
      (! return {result}) ]
  
  let rec unwrap_usize (self_: t_Result_usize_usize) (return (x: UInt64.t)) =
    {[@expl:unwrap requires] exists t: UInt64.t. self_ = Ok t}
    any [ return (result: UInt64.t) -> {Ok result = self_} (! return {result}) ]
  
  meta "compute_max_steps" 1000000
  
  meta "select_lsinst" "all"
  
  let rec binary_search (s: Slice64.slice UInt32.t) (return (x: UInt64.t)) =
    {[@expl:binary_search requires #0] forall i: int. 0 <= i /\ i < Seq.length (Slice64.view s)
      -> UInt32.t'int (index_slice_u32 s i) = i}
    {[@expl:binary_search requires #1] Seq.length (Slice64.view s) = 5}
    (! bb0
    [ bb0 = s0
      [ s0 = bb0'0
        [ bb0'0 = s0'0 [ s0'0 = [ &_0'0 <- (2: UInt32.t) ] s1'0 | s1'0 = _const_ret {_0'0} ] ]
        [ & _0'0: UInt32.t = Any.any_l () ] [ _const_ret (_const: UInt32.t) -> [ &_15 <- _const ] s1 ]
      | s1 = [ &_10 <- _15 ] s2
      | s2 = binary_search_u32 {s} {_10} (fun (_ret: t_Result_usize_usize) -> [ &_7 <- _ret ] s3)
      | s3 = bb1 ]
    | bb1 = s0 [ s0 = unwrap_usize {_7} (fun (_ret: UInt64.t) -> [ &ix <- _ret ] s1) | s1 = bb2 ]
    | bb2 = s0 [ s0 = {[@expl:assertion] UInt64.t'int ix < 5} s1 | s1 = [ &_0 <- ix ] s2 | s2 = return {_0} ] ]
    [ & _0: UInt64.t = Any.any_l ()
    | & s: Slice64.slice UInt32.t = s
    | & ix: UInt64.t = Any.any_l ()
    | & _7: t_Result_usize_usize = Any.any_l ()
    | & _10: UInt32.t = Any.any_l ()
    | & _15: UInt32.t = Any.any_l () ]) [ return (result: UInt64.t) -> (! return {result}) ]
end
