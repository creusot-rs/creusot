module M_f
  use creusot.prelude.Any
  use creusot.int.UInt32
  
  meta "compute_max_steps" 1000000
  
  meta "select_lsinst" "all"
  
  let rec f (return (x: ())) = (! bb0
    [ bb0 = return {_ret} ] [ & _ret: () = Any.any_l () ])
    [ return (result: ()) -> {[@stop_split] [@expl:f ensures] forall _x: UInt32.t. true
        /\ true /\ true /\ true /\ true /\ true /\ true /\ true /\ true}
      (! return {result}) ]
end
module M_omg
  use int.Int
  
  meta "compute_max_steps" 1000000
  
  meta "select_lsinst" "all"
  
  constant a : int
  
  constant b : int
  
  predicate omg (a: int) (b: int)
  
  goal vc_omg: [@stop_split] [@expl:omg ensures] true
end
