module M_uses_generic
  use creusot.int.UInt32
  use creusot.prelude.Any
  
  type t_T
  
  predicate inv_T (_1: t_T)
  
  predicate precondition_from_b (self: ()) (args: t_T)
  
  axiom precondition_fndef: forall args: t_T [precondition_from_b () args]. (let x = args in inv_T x)
      -> precondition_from_b () args
  
  predicate postcondition_once_from_b (self: ()) (args: t_T) (result: UInt32.t)
  
  axiom postcondition_fndef:
    forall args: t_T, res: UInt32.t [postcondition_once_from_b () args res]. postcondition_once_from_b () args res
      -> (let x = args in true)
  
  let rec from_b_T (x: t_T) (return (x'0: UInt32.t)) = {[@expl:from_b requires] precondition_from_b () x}
    any [ return (result: UInt32.t) -> {postcondition_once_from_b () x result} (! return {result}) ]
  
  meta "compute_max_steps" 1000000
  
  meta "select_lsinst" "all"
  
  let rec uses_generic_T (b: t_T) (return (x: UInt32.t)) = {[@expl:uses_generic 'b' type invariant] inv_T b}
    (! bb0
    [ bb0 = s0 [ s0 = from_b_T {b} (fun (_x: UInt32.t) -> [ &_ret <- _x ] s1) | s1 = bb2 ] | bb2 = return {_ret} ]
    [ & _ret: UInt32.t = Any.any_l () | & b: t_T = b ]) [ return (result: UInt32.t) -> (! return {result}) ]
end
