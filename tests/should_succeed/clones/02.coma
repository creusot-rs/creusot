module M_program
  use creusot.prelude.Any
  
  predicate simple = true
  
  predicate uses_simple = simple
  
  meta "compute_max_steps" 1000000
  
  meta "select_lsinst" "all"
  
  let rec program (return (x: ())) = {[@expl:program requires] uses_simple}
    (! bb0
    [ bb0 = return {_ret} ] [ & _ret: () = Any.any_l () ])
    [ return (result: ()) -> {[@expl:program ensures] simple} (! return {result}) ]
end
