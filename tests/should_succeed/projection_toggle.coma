module M_proj_toggle
  use creusot.prelude.MutBorrow
  use creusot.prelude.Any
  
  type t_T
  
  predicate inv_T (_1: t_T)
  
  predicate invariant_ref_T [@inline:trivial] (self: MutBorrow.t t_T) = inv_T self.current /\ inv_T self.final
  
  meta "rewrite_def" predicate invariant_ref_T
  
  predicate inv_ref_T [@inline:trivial] (_1: MutBorrow.t t_T) = invariant_ref_T _1
  
  meta "rewrite_def" predicate inv_ref_T
  
  predicate resolve_ref_T [@inline:trivial] (_1: MutBorrow.t t_T) = _1.final = _1.current
  
  meta "rewrite_def" predicate resolve_ref_T
  
  meta "compute_max_steps" 1000000
  
  meta "select_lsinst" "all"
  
  let rec proj_toggle_T (toggle: bool) (a: MutBorrow.t t_T) (b: MutBorrow.t t_T) (return (x: MutBorrow.t t_T)) =
    {[@expl:proj_toggle 'a' type invariant] inv_ref_T a}
    {[@expl:proj_toggle 'b' type invariant] inv_ref_T b}
    (! bb0
    [ bb0 = any [ br0 -> {toggle = false} (! bb2) | br1 -> {toggle} (! bb1) ]
    | bb1 = s0
      [ s0 = {[@expl:type invariant] inv_ref_T b} s1
      | s1 = -{resolve_ref_T b}- s2
      | s2 = {inv_T a.current}
        MutBorrow.borrow_final <t_T> {a.current} {MutBorrow.get_id a}
          (fun (_bor: MutBorrow.t t_T) ->
            [ &_11 <- _bor ] -{inv_T _bor.final}-
            [ &a <- { a with current = _bor.final } ] s3)
      | s3 = {inv_T _11.current}
        MutBorrow.borrow_final <t_T> {_11.current} {MutBorrow.get_id _11}
          (fun (_bor: MutBorrow.t t_T) ->
            [ &_9 <- _bor ] -{inv_T _bor.final}-
            [ &_11 <- { _11 with current = _bor.final } ] s4)
      | s4 = {[@expl:type invariant] inv_ref_T _11} s5
      | s5 = -{resolve_ref_T _11}- s6
      | s6 = bb3 ]
    | bb2 = s0
      [ s0 = {[@expl:type invariant] inv_ref_T a} s1
      | s1 = -{resolve_ref_T a}- s2
      | s2 = {inv_T b.current}
        MutBorrow.borrow_final <t_T> {b.current} {MutBorrow.get_id b}
          (fun (_bor: MutBorrow.t t_T) ->
            [ &_9 <- _bor ] -{inv_T _bor.final}-
            [ &b <- { b with current = _bor.final } ] s3)
      | s3 = bb3 ]
    | bb3 = s0
      [ s0 = {inv_T _9.current}
        MutBorrow.borrow_final <t_T> {_9.current} {MutBorrow.get_id _9}
          (fun (_bor: MutBorrow.t t_T) ->
            [ &_4 <- _bor ] -{inv_T _bor.final}-
            [ &_9 <- { _9 with current = _bor.final } ] s1)
      | s1 = {[@expl:type invariant] inv_ref_T _9} s2
      | s2 = -{resolve_ref_T _9}- s3
      | s3 = {inv_T _4.current}
        MutBorrow.borrow_final <t_T> {_4.current} {MutBorrow.get_id _4}
          (fun (_bor: MutBorrow.t t_T) ->
            [ &_ret <- _bor ] -{inv_T _bor.final}-
            [ &_4 <- { _4 with current = _bor.final } ] s4)
      | s4 = {[@expl:type invariant] inv_ref_T _4} s5
      | s5 = -{resolve_ref_T _4}- s6
      | s6 = {[@expl:type invariant] inv_ref_T b} s7
      | s7 = -{resolve_ref_T b}- s8
      | s8 = {[@expl:type invariant] inv_ref_T a} s9
      | s9 = -{resolve_ref_T a}- s10
      | s10 = return {_ret} ] ]
    [ & _ret: MutBorrow.t t_T = Any.any_l ()
    | & toggle: bool = toggle
    | & a: MutBorrow.t t_T = a
    | & b: MutBorrow.t t_T = b
    | & _4: MutBorrow.t t_T = Any.any_l ()
    | & _9: MutBorrow.t t_T = Any.any_l ()
    | & _11: MutBorrow.t t_T = Any.any_l () ])
    [ return (result: MutBorrow.t t_T) -> {[@expl:proj_toggle result type invariant] inv_ref_T result}
      {[@expl:proj_toggle ensures] if toggle then
        result = a /\ b.final = b.current
      else
        result = b /\ a.final = a.current
      }
      (! return {result}) ]
end
