module M_closure_param
  use creusot.int.UInt32
  use creusot.prelude.Any
  
  type t_F
  
  predicate inv_F (_1: t_F)
  
  predicate resolve_F (_1: t_F)
  
  predicate invariant_ref_F [@inline:trivial] (self: t_F) = inv_F self
  
  meta "rewrite_def" predicate invariant_ref_F
  
  predicate inv_ref_F [@inline:trivial] (_1: t_F) = invariant_ref_F _1
  
  meta "rewrite_def" predicate inv_ref_F
  
  predicate precondition_F (self: t_F) (args: UInt32.t)
  
  predicate postcondition_once_F (self: t_F) (args: UInt32.t) (result: ())
  
  predicate postcondition_mut_F (self: t_F) (args: UInt32.t) (result_state: t_F) (result: ())
  
  function fn_mut_once_F (self: t_F) (args: UInt32.t) (res: ()) : ()
  
  axiom fn_mut_once_F_spec: forall self: t_F, args: UInt32.t, res: (). postcondition_once_F self args res
      = (exists res_state: t_F. postcondition_mut_F self args res_state res /\ resolve_F res_state)
  
  predicate hist_inv_F (self: t_F) (result_state: t_F)
  
  function hist_inv_trans_F (self: t_F) (b: t_F) (c: t_F) : ()
  
  axiom hist_inv_trans_F_spec: forall self: t_F, b: t_F, c: t_F. hist_inv_F self b
      -> hist_inv_F b c -> hist_inv_F self c
  
  function hist_inv_refl_F (self: t_F) : ()
  
  axiom hist_inv_refl_F_spec: forall self: t_F. hist_inv_F self self
  
  function postcondition_mut_hist_inv_F (self: t_F) (args: UInt32.t) (res_state: t_F) (res: ()) : ()
  
  axiom postcondition_mut_hist_inv_F_spec:
    forall self: t_F, args: UInt32.t, res_state: t_F, res: (). postcondition_mut_F self args res_state res
      -> hist_inv_F self res_state
  
  function fn_hist_inv_F (self: t_F) (res_state: t_F) : ()
  
  axiom fn_hist_inv_F_spec: forall self: t_F, res_state: t_F. hist_inv_F self res_state = (self = res_state)
  
  predicate postcondition_F (self: t_F) (args: UInt32.t) (result: ())
  
  function fn_once_F (self: t_F) (args: UInt32.t) (res: ()) : ()
  
  axiom fn_once_F_spec: forall self: t_F, args: UInt32.t, res: (). postcondition_once_F self args res
      = (postcondition_F self args res /\ resolve_F self)
  
  function fn_mut_F (self: t_F) (args: UInt32.t) (res_state: t_F) (res: ()) : ()
  
  axiom fn_mut_F_spec:
    forall self: t_F, args: UInt32.t, res_state: t_F, res: (). postcondition_mut_F self args res_state res
      = (postcondition_F self args res /\ self = res_state)
  
  let rec call_F (self_: t_F) (arg: UInt32.t) (return (x: ())) = {[@expl:call 'self_' type invariant] inv_ref_F self_}
    {[@expl:call requires] precondition_F self_ arg}
    any [ return (result: ()) -> {postcondition_F self_ arg result} (! return {result}) ]
  
  meta "compute_max_steps" 1000000
  
  meta "select_lsinst" "all"
  
  let rec closure_param_F (f: t_F) (return (x: ())) = {[@expl:closure_param 'f' type invariant] inv_F f}
    (! bb0
    [ bb0 = s0
      [ s0 = {[@expl:type invariant] inv_F f} s1
      | s1 = -{resolve_F f}- s2
      | s2 = [ &_3 <- (0: UInt32.t) ] s3
      | s3 = call_F {f} {_3} (fun (_x: ()) -> [ &_ret <- _x ] s4)
      | s4 = return {_ret} ] ] [ & _ret: () = Any.any_l () | & f: t_F = f | & _3: UInt32.t = Any.any_l () ])
    [ return (result: ()) -> (! return {result}) ]
end
