    [ bb0 = s0 [ s0 = [ &bor_snap <- bor ] s1 | s1 = bb1 ]
    | bb1 = s0
      [ s0 = MutBorrow.borrow_mut <Int32.t> {bor.current}
          (fun (_bor: MutBorrow.t Int32.t) -> [ &b1 <- _bor ] [ &bor <- { bor with current = _bor.final } ] s1)
      | s1 = [ &b1_snap <- b1 ] s2
      | s2 = bb2 ]
    | bb2 = s0 [ s0 = inner (fun (_x: Int32.t) -> [ &_9 <- _x ] s1) | s1 = bb3 ]
    | bb3 = s0
